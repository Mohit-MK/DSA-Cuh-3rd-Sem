#include <stdio.h>
int main(){
  int r, c, Element, count=0;
  scanf("%d %d", &r, &c);
  int arr[r][c];
  for(int i=0; i<r; i++){
    for(int j=0; j<c; j++){
      scanf("%d", &arr[i][j]);}}
  scanf("%d", &Element);
  for(int i=0; i<r; i++){
    for(int j=0; j<c; j++){
      if(arr[i][j] == Element){
        printf("%d, %d", i, j);
        count++;}}}
  if(count==0)
    printf("Not found \n");
return 0;
}
